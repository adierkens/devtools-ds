{
  "name": "diagnostic",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/diagnostic/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/diagnostic/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/diagnostic",
        "main": "packages/diagnostic/src/index.ts",
        "tsConfig": "packages/diagnostic/tsconfig.lib.json",
        "assets": ["packages/diagnostic/*.md"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/packages/diagnostic"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/diagnostic"
      }
    }
  }
}
